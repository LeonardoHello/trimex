{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^react$",
    "",
    "^next(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@/|@payload-config)(.*)$",
    "^[.]"
  ],
  "singleQuote": false,
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true
}
